  "main": "index.js",
  "scripts": {
    "start": "npx expo start --clear --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ignore-pattern node_modules/  --ignore-pattern .expo/",
